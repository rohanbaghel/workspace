package com.capgemini.generic.model;

import java.util.ArrayList;

public class Collections {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		ArrayList<Integer> iList= new ArrayList<Integer>();
		iList.add(10);
		iList.add(20);
		iList.add(30);
		iList.add(40);
	
		iList.add(2,10);
		iList.remove(2);
		iList.set(3, 120);
		for(int i:iList){
			System.out.print(i+" ");
		}
	}

}
